{
    "debug": false,
    "hostname": "",
    "noBuiltin": false,
    "ip": "",
    "plugin": {
        "enabled": false,
        "signingKeys": [],
        "authorizedKeys": "authorized_keys",
        "update": "agent-update.conf",
        "autoSync": true,
        "git": "http://satori:PASSWD@DOMAIN/plugin",
        "checkoutPath": "/var/lib/satori/plugin",
        "subDir": "plugin",
        "logs": "/var/log/satori"
    },
    "master": {
        "enabled": true,
        "addr": "INTERNAL_DOMAIN:6040",
        "interval": 60,
        "timeout": 1000
    },
    "transfer": {
        "enabled": true,
        "addrs": [
            "INTERNAL_DOMAIN:8433"
        ],
        "interval": 60,
        "timeout": 1000
    },
    "http": {
        "enabled": true,
        "listen": ":1988"
    },
    "collector": {
        "ifacePrefix": ["eth", "em"]
    },
    "addTags":{
      "region": "default"
    },
    "ignore": [
        ["^df\\.", "mount", ".*/(docker|ureadahead|netns)/.*$"]
    ]
}
